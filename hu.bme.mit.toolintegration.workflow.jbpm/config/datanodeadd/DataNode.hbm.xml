<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-access="field" >
  <subclass name="hu.bme.mit.toolintegration.workflow.jbpm.dataflow.DataNode" 
            extends="org.jbpm.graph.def.Node"
            discriminator-value="W">
			
	<property name="variableName" column="VARIABLENAME_"/>		
	<property name="variableType" column="VARIABLETYPE_"/>
	<property name="variableValue" column="VARIABLEVALUE_"/>
	<property name="isTransient" column="ISTRANSIENT_"/>
	<property name="dataNodeId" column="DATANODEID_"/>
	<property name="dataType" column="DATATYPE_"/>		
	<property name="toolId" column="TOOLID_"/>
  </subclass>
</hibernate-mapping> 